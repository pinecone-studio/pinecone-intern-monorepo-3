{
  "name": "instagram-backend",
  "$schema": "../../../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "apps/2i/instagram/backend",
  "projectType": "application",
  "targets": {
    "build": {
      "executor": "@nx/next:build",
      "outputs": ["{options.outputPath}"],
      "defaultConfiguration": "production",
      "options": {
        "outputPath": "dist/apps/2i/instagram/backend"
      },
      "configurations": {
        "development": {
          "outputPath": "dist/apps/2i/instagram/backend"
        }
      },
      "dependsOn": [
        {
          "target": "codegen",
          "params": "forward"
        }
      ]
    },
    "serve": {
      "executor": "@nx/next:server",
      "defaultConfiguration": "development",
      "options": {
        "buildTarget": "instagram-backend:build",
        "dev": true
      },
      "configurations": {
        "development": {
          "buildTarget": "instagram-backend:build:development",
          "dev": true
        },
        "production": {
          "buildTarget": "instagram-backend:build:production",
          "dev": false
        }
      },
      "dependsOn": [
        {
          "target": "codegen",
          "params": "forward"
        }
      ]
    },
    "test": {
      "executor": "@nx/jest:jest",
      "outputs": ["{workspaceRoot}/coverage/{projectRoot}"],
      "options": {
        "jestConfig": "apps/2i/instagram/backend/jest.config.ts",
        "passWithNoTests": true,
        "codeCoverage": true
      },
      "dependsOn": [
        {
          "target": "codegen",
          "params": "forward"
        }
      ]
    },
    "get-secrets:dev": {
      "executor": "@pinecone-intern-monorepo/secrets:get",
      "defaultConfiguration": "development",
      "configurations": {
        "development": {
          "groups": ["Instagram-2I-2025-BE"],
          "env": "dev"
        }
      }
    },
    "get-secrets:testing": {
      "executor": "@pinecone-intern-monorepo/secrets:get",
      "defaultConfiguration": "testing",
      "configurations": {
        "testing": {
          "groups": ["Instagram-2I-2025-BE"],
          "env": "test"
        }
      }
    },
    "get-secrets:prod": {
      "executor": "@pinecone-intern-monorepo/secrets:get",
      "defaultConfiguration": "prod",
      "configurations": {
        "prod": {
          "groups": ["Instagram-2I-2025-BE"],
          "env": "prod"
        }
      }
    },
    "codegen": {
      "executor": "nx:run-commands",
      "options": {
        "commands": ["graphql-codegen --config ./apps/2i/instagram/backend/codegen.yml"]
      }
    },
    "export": {
      "executor": "@nx/next:export",
      "options": {
        "buildTarget": "instagram-backend:build:production",
        "outputPath": ".vercel/output/static"
      }
    },
    "export-env": {
      "executor": "nx:run-commands",
      "options": {
        "commands": ["export $(cat apps/2i/instagram/backend/.env | xargs)"],
        "parallel": false
      }
    },
    "lint": {
      "executor": "@nx/eslint:lint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": ["apps/2i/instagram/backend/**/*.{ts,tsx,js,jsx}"]
      }
    },
    "clean": {
      "executor": "nx:run-commands",
      "options": {
        "commands": ["rm -rf dist/apps/2i/instagram/backend", "rm -rf .vercel"]
      }
    },
    "preview": {
      "executor": "nx:run-commands",
      "options": {
        "commands": [
          "npx dotenv -v VERCEL_ORG_ID=Ek2AAina6XqHTBMJTdK1bq5U -v VERCEL_PROJECT_ID=prj_kHRfRvviNAcAuBZQfGyl5HhViRtE -- vercel pull --yes --environment=preview --token=0nMLr6qC8OPvIR4GGd1M23rK",
          "npx env-cmd -f apps/2i/instagram/backend/.env vercel build --token=0nMLr6qC8OPvIR4GGd1M23rK",
          "npx dotenv -v VERCEL_ORG_ID=Ek2AAina6XqHTBMJTdK1bq5U -v VERCEL_PROJECT_ID=prj_kHRfRvviNAcAuBZQfGyl5HhViRtE -- vercel --archive=tgz --prebuilt --token=0nMLr6qC8OPvIR4GGd1M23rK"
        ],
        "parallel": false
      },
      "dependsOn": [
        {
          "target": "clean"
        },
        {
          "target": "get-secrets:testing"
        },
        {
          "target": "export-env"
        },
        {
          "target": "codegen",
          "params": "forward"
        }
      ]
    },
    "deploy-testing": {
      "executor": "nx:run-commands",
      "options": {
        "commands": [
          "npx dotenv -v VERCEL_ORG_ID=Ek2AAina6XqHTBMJTdK1bq5U -v VERCEL_PROJECT_ID=prj_kHRfRvviNAcAuBZQfGyl5HhViRtE -- vercel pull --yes --environment=production --token=0nMLr6qC8OPvIR4GGd1M23rK",
          "vercel build --prod --token=0nMLr6qC8OPvIR4GGd1M23rK",
          "npx dotenv -v VERCEL_ORG_ID=Ek2AAina6XqHTBMJTdK1bq5U -v VERCEL_PROJECT_ID=prj_kHRfRvviNAcAuBZQfGyl5HhViRtE -- vercel --prebuilt --prod --token=0nMLr6qC8OPvIR4GGd1M23rK"
        ],
        "parallel": false
      },
      "dependsOn": [
        {
          "target": "clean"
        },
        {
          "target": "get-secrets:testing"
        },
        {
          "target": "export-env"
        },
        {
          "target": "codegen",
          "params": "forward"
        }
      ]
    },
    "deploy-prod": {
      "executor": "nx:run-commands",
      "options": {
        "commands": [
          "npx dotenv -v VERCEL_ORG_ID=... -v VERCEL_PROJECT_ID=... -- vercel pull --yes --environment=production --token=$VERCEL_TOKEN",
          "yarn nx run instagram-backend:export",
          "vercel --prebuilt --prod --token=$VERCEL_TOKEN"
        ],
        "parallel": false
      },
      "dependsOn": [
        {
          "target": "clean"
        },
        {
          "target": "get-secrets:prod"
        },
        {
          "target": "export-env"
        },
        {
          "target": "codegen",
          "params": "forward"
        }
      ]
    }
  },
  "tags": []
}
