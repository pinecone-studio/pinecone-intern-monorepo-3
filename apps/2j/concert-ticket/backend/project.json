{
  "name": "concert-ticket-backend",
  "$schema": "../../../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "apps/2j/concert-ticket/backend",
  "projectType": "application",
  "targets": {
    "get-secrets:dev": {
      "executor": "@pinecone-intern-monorepo/secrets:get",
      "defaultConfiguration": "development",
      "configurations": {
        "development": {
          "groups": ["2J-ticket-backend-2025"],
          "env": "dev"
        }
      }
    },
    "build": {
      "executor": "@nx/next:build",
      "outputs": ["{options.outputPath}"],
      "defaultConfiguration": "production",
      "options": {
        "outputPath": "dist/apps/2j/concert-ticket/backend"
      },
      "configurations": {
        "development": {
          "outputPath": "dist/apps/2j/concert-ticket/backend"
        }
      },
      "dependsOn": [
        {
          "target": "codegen",
          "params": "forward"
        }
      ]
    },
    "serve": {
      "executor": "@nx/next:server",
      "defaultConfiguration": "development",
      "options": {
        "buildTarget": "concert-ticket-backend:build",
        "dev": true
      },
      "configurations": {
        "development": {
          "buildTarget": "concert-ticket-backend:build:development",
          "dev": true
        },
        "production": {
          "buildTarget": "concert-ticket-backend:build:production",
          "dev": false
        }
      },
      "dependsOn": [
        {
          "target": "codegen",
          "params": "forward"
        }
      ]
    },
    "test": {
      "executor": "@nx/jest:jest",
      "outputs": ["{workspaceRoot}/coverage/{projectRoot}"],
      "options": {
        "jestConfig": "apps/2j/concert-ticket/backend/jest.config.ts",
        "passWithNoTests": true,
        "codeCoverage": true
      },
      "dependsOn": [
        {
          "target": "codegen",
          "params": "forward"
        }
      ]
    },
    "get-secrets:testing": {
      "executor": "@pinecone-intern-monorepo/secrets:get",
      "defaultConfiguration": "testing",
      "configurations": {
        "testing": {
          "groups": ["concert-ticket-backend"],
          "env": "test"
        }
      }
    },
    "get-secrets:prod": {
      "executor": "@pinecone-intern-monorepo/secrets:get",
      "defaultConfiguration": "prod",
      "configurations": {
        "prod": {
          "groups": ["concert-ticket-backend"],
          "env": "prod"
        }
      }
    },
    "codegen": {
      "executor": "nx:run-commands",
      "options": {
        "commands": ["graphql-codegen --config ./apps/2j/concert-ticket/backend/codegen.ts"]
      }
    },
    "export": {
      "executor": "@nx/next:export",
      "options": {
        "buildTarget": "concert-ticket-backend:build:production"
      }
    },
    "export-env": {
      "executor": "nx:run-commands",
      "options": {
        "commands": ["export $(cat apps/2j/concert-ticket/backend/.env | xargs)"],
        "parallel": false
      }
    },
    "lint": {
      "executor": "@nx/eslint:lint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": ["apps/2j/concert-ticket/backend/**/*.{ts,tsx,js,jsx}"]
      }
    },
    "clean": {
      "executor": "nx:run-commands",
      "options": {
        "commands": ["rm -rf dist/apps/2j/concert-ticket/backend", "rm -rf .vercel"]
      }
    },
    "preview": {
      "executor": "nx:run-commands",
      "options": {
        "cwd": "apps/2j/concert-ticket/backend",
        "commands": [
          "rm -rf .vercel",
          "VERCEL_ORG_ID=$VERCEL_ORG_ID VERCEL_PROJECT_ID=$VERCEL_PROJECT_ID vercel pull --yes --environment=preview --token=$VERCEL_TOKEN",
          "npx env-cmd -f ./.env vercel build --token=$VERCEL_TOKEN",
          "VERCEL_ORG_ID=$VERCEL_ORG_ID VERCEL_PROJECT_ID=$VERCEL_PROJECT_ID vercel --archive=tgz --prebuilt --token=$VERCEL_TOKEN"
        ],
        "parallel": false
      },
      "dependsOn": [{ "target": "clean" }, { "target": "get-secrets:testing" }, { "target": "export-env" }, { "target": "codegen", "params": "forward" }]
    },
    "deploy-testing": {
      "executor": "nx:run-commands",
      "options": {
        "commands": [
          "nx codegen concert-ticket-backend",
          "mkdir -p .vercel && cp -r ./apps/2j/concert-ticket/backend/vercel.config.json .vercel/project.json",
          "npx env-cmd -f apps/2j/concert-ticket/backend/.env sh -c 'vercel build --prod --token=\"$VERCEL_TOKEN\"'",
          "npx env-cmd -f apps/2j/concert-ticket/backend/.env sh -c 'vercel --prebuilt --prod --yes --token=\"$VERCEL_TOKEN\"'"
        ],
        "parallel": false
      },
      "dependsOn": [
        {
          "target": "clean"
        },
        {
          "target": "get-secrets:dev"
        }
      ]
    },
    "deploy-prod": {
      "executor": "nx:run-commands",
      "options": {
        "commands": [
          "npx dotenv -v VERCEL_ORG_ID=$VERCEL_ORG_ID -v VERCEL_PROJECT_ID= -- vercel pull --yes --environment=production --token=$VERCEL_TOKEN",
          "vercel build --prod --token=$VERCEL_TOKEN",
          "npx dotenv -v VERCEL_ORG_ID=$VERCEL_ORG_ID -v VERCEL_PROJECT_ID= -- vercel --prebuilt --prod --token=$VERCEL_TOKEN"
        ],
        "parallel": false
      },
      "dependsOn": [
        {
          "target": "clean"
        },
        {
          "target": "get-secrets:prod"
        },
        {
          "target": "export-env"
        },
        {
          "target": "codegen",
          "params": "forward"
        }
      ]
    }
  },
  "tags": []
}
