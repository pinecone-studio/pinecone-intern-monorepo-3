{
  "extends": ["../../../../.eslintrc.json"],
  "ignorePatterns": [
    "src/generated/**",
    "dist/**",
    "build/**"
  ],
  "overrides": [
    {
      "files": ["**/*.ts", "**/*.tsx", "**/*.js", "**/*.jsx"],
      "rules": {
        "max-lines": "off",
        "complexity": "off",
        "@typescript-eslint/no-explicit-any": "warn",
        "no-unused-vars": ["error", { "argsIgnorePattern": "^_", "varsIgnorePattern": "^_" }],
        "no-html-link-for-pages": "off",
        "@next/next/no-html-link-for-pages": "off"
      }
    }
  ]
}
